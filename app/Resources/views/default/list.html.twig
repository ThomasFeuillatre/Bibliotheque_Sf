{% extends 'base.html.twig' %}


{% block body %}
    <h3>Liste des livres</h3>
    {% for book in  listBook %}
        <div>
            <h5>{{ book.title }}</h5>
            <p>{{ book.summary }}</p>
        </div>
    {% endfor %}

    <h3>Liste des lecteurs</h3>
    {% for reader in listReader %}
        <div>
            <p>{{ reader.firstname }} {{ reader.name }}</p>
        </div>
    {% endfor %}
{% endblock %}

{% block stylesheets %}
{% endblock %}
